{
  "name": "@auxilin/node-mongo",
  "version": "0.1.0",
  "author": "Auxilin",
  "description": "MongoDB wrapper for Node.JS",
  "private": false,
  "main": "src/index.js",
  "scripts": {
    "test": "run-s test:*",
    "test:eslint": "eslint ./",
    "test:mocha": "NODE_ENV=test mocha --exit --recursive -c -R spec 'src/*.spec.js'",
    "coveralls": "nyc npm run test:mocha && nyc report --reporter=text-lcov | coveralls",    
    "add-contributor": "all-contributors add",
    "generate-contributors": "all-contributors generate"
  },
  "keywords": [
    "mongo",
    "monk",
    "auxilin"
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.11",
    "monk": "6.0.6"
  },
  "devDependencies": {
    "@auxilin/eslint-config": "0.1.1",
    "all-contributors-cli": "5.4.0",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "eslint": "5.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "joi": "13.7.0",
    "jsonschema": "1.2.4",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "npm-run-all": "4.1.3",
    "nyc": "13.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auxilincom/node-mongo.git"
  }
}
